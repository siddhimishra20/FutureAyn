    {
      "parameters": {
        "jsCode": "const raw = items[0].json.output;\n\nconst cleanedText = raw\n  .replace(/\\\\n/g, '\\n')                              // Convert escaped newlines\n  .replace(/\\*\\*/g, '')                               // Remove markdown bold\n  .replace(/\\[([^\\]]+)\\]\\((.*?)\\)/g, '$1 ($2)')       // Convert [title](url) â†’ title (url)\n  .replace(/- /g, '\\n- ')                             // Ensure dash bullets start new lines\n  .replace(/\\n{2,}/g, '\\n\\n')                         // Keep clean double newlines for spacing\n  .trim();\n\nreturn [\n  {\n    json: {\n      formatted_output: cleanedText\n    }\n  }\n];"
      },
      "type": "n8n-nodes-base.code",
